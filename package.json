{
  "name": "svs",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "start": "ts-node-esm server.ts",
    "build": "npm run build:client && npm run build:server",
    "build:client": "vue-tsc --noEmit && vite build --ssrManifest --outDir dist/client",
    "build:server": "vite build --ssr src/entry-server.ts --outDir dist/server",
    "build:static": "vite build --ssrManifest --outDir dist/static && npm run build:server && ts-node-esm prerender.ts",
    "serve": "NODE_ENV=production ts-node-esm server.ts",
    "update": "npm-check --skip-unused -u"
  },
  "dependencies": {
    "@vueuse/core": "^9.3.0",
    "@vueuse/integrations": "^9.3.0",
    "vue": "^3.2.40",
    "vue-router": "^4.1.5"
  },
  "devDependencies": {
    "@babel/types": "^7.19.3",
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.14",
    "@types/node": "^18.7.23",
    "@vitejs/plugin-vue": "^3.1.0",
    "compression": "^1.7.4",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jsdoc": "^39.3.6",
    "eslint-plugin-vue": "^9.5.1",
    "express": "^4.18.1",
    "npm-check": "^6.0.1",
    "prettier": "^2.7.1",
    "sass": "^1.55.0",
    "serve-static": "^1.15.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4",
    "unplugin-vue-components": "^0.22.7",
    "vite": "^3.1.4",
    "vue-tsc": "^0.40.13"
  }
}
