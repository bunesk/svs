{
  "name": "svs",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "engines": {
    "node": ">=16.14",
    "npm": ">=8.3"
  },
  "scripts": {
    "start": "ts-node-esm src/server/index.ts",
    "build": "npm run build:client && npm run build:server",
    "build:client": "vue-tsc --noEmit && vite build --ssrManifest --outDir dist/client",
    "build:server": "vite build --ssr src/server/entry-server.ts --outDir dist/server",
    "serve": "NODE_ENV=production ts-node-esm src/server/index.ts",
    "update": "npm-check --skip-unused -u"
  },
  "dependencies": {
    "@vueuse/core": "^9.5.0",
    "@vueuse/integrations": "^9.5.0",
    "express": "^4.18.2",
    "ldapjs": "^2.3.3",
    "primeicons": "^6.0.1",
    "primevue": "^3.18.1",
    "sequelize": "^6.25.5",
    "vue": "^3.2.41",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@babel/types": "^7.20.2",
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.14",
    "@types/ldapjs": "^2.2.5",
    "@types/mysql": "^2.15.21",
    "@types/node": "^18.11.9",
    "@vitejs/plugin-vue": "^3.2.0",
    "compression": "^1.7.4",
    "dotenv": "^16.0.3",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jsdoc": "^39.6.2",
    "eslint-plugin-vue": "^9.7.0",
    "express-jwt": "^7.7.7",
    "jsonwebtoken": "^8.5.1",
    "mariadb": "^3.0.2",
    "npm-check": "^6.0.1",
    "pg-hstore": "^2.3.4",
    "prettier": "^2.7.1",
    "sass": "^1.56.1",
    "serve-static": "^1.15.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4",
    "universal-cookie": "^4.0.4",
    "unplugin-vue-components": "^0.22.9",
    "vite": "^3.2.3",
    "vite-plugin-node-polyfills": "^0.5.0",
    "vite-plugin-pages": "^0.28.0",
    "vue-tsc": "^1.0.9"
  }
}
